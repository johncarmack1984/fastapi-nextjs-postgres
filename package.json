{
  "name": "auxhealth-interview",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "precodegen": "node ./lib/api/pre-codegen.mjs",
    "codegen": "openapi-rq --input ./lib/api/openapi.json --output ./lib/api/client --client fetch --useUnionTypes --exportSchemas true",
    "db-codegen": "kysely-codegen --out-file ./lib/db/db.d.ts --include-pattern \"auxhealth_*\"",
    "fastapi-dev": "pip3 install -q -r requirements.txt && python3 -m uvicorn api.index:app --reload",
    "next-dev": "next dev --turbo",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@mdx-js/mdx": "^3.0.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.29.0",
    "@tanstack/react-query-next-experimental": "^5.29.0",
    "@tanstack/react-query-persist-client": "^5.29.0",
    "@vercel/postgres-kysely": "^0.8.0",
    "autoprefixer": "10.4.19",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "hast": "^1.0.0",
    "idb-keyval": "^6.2.1",
    "javascript-time-ago": "^2.5.9",
    "kysely": "^0.27.3",
    "lodash": "^4.17.21",
    "lucide-react": "^0.365.0",
    "next": "canary",
    "next-mdx-remote": "^4.4.1",
    "next-mdx-remote-client": "^1.0.2",
    "pg": "^8.11.5",
    "postcss": "8.4.38",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.2",
    "react-intersection-observer": "^9.8.2",
    "react-time-ago": "^7.3.1",
    "recma-mdx-change-props": "^1.1.0",
    "recma-mdx-escape-missing-components": "^1.1.0",
    "rehype-highlight": "^7.0.0",
    "rehype-raw": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-emoji": "^4.0.1",
    "remark-flexible-code-titles": "^1.2.0",
    "remark-flexible-containers": "^1.2.1",
    "remark-flexible-markers": "^1.2.1",
    "remark-flexible-paragraphs": "^1.2.0",
    "remark-flexible-toc": "^1.1.0",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "unified": "^11.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@7nohe/openapi-react-query-codegen": "^0.5.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@tanstack/react-query-devtools": "^5.29.0",
    "@types/lodash": "^4.17.0",
    "@types/mdx": "^2.0.12",
    "@types/node": "20.12.5",
    "@types/react": "latest",
    "@types/react-dom": "latest",
